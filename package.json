{
  "name": "nuxtstarterkit",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.8.1",
  "scripts": {
    "dev": "nuxt dev",
    "dev:remote": "nuxt dev --remote",
    "build": "NODE_OPTIONS=--max_old_space_size=8192 nuxt build",
    "preview": "npx nuxthub preview",
    "deploy": "npx nuxthub deploy",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e": "playwright test",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "generate:api": "node --experimental-modules scripts/generateApi.js"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@iconify-json/lucide": "^1.2.56",
    "@iconify-json/simple-icons": "^1.2.42",
    "@nuxt/content": "3.6.3",
    "@nuxt/eslint": "^1.5.2",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/image": "1.10.0",
    "@nuxt/ui-pro": "3.2.0",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/i18n": "9.5.6",
    "@nuxtjs/seo": "3.1.0",
    "@polar-sh/nuxt": "0.3.3",
    "@t3-oss/env-nuxt": "^0.13.8",
    "@unovis/ts": "^1.5.2",
    "@unovis/vue": "^1.5.2",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue-email/components": "^0.0.21",
    "@vue-email/render": "^0.0.9",
    "@vueuse/core": "^13.5.0",
    "@vueuse/nuxt": "13.5.0",
    "canvas-confetti": "^1.9.3",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.2",
    "nanoid": "^5.1.5",
    "nuxt": "^3.17.6",
    "nuxt-auth-utils": "0.5.20",
    "nuxt-llms": "^0.1.3",
    "nuxt-security": "2.2.0",
    "nuxt-umami": "3.2.0",
    "uuid": "11.1.0",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@adonisjs/hash": "^9.1.1",
    "@inquirer/prompts": "^7.6.0",
    "@nuxt/eslint-config": "^1.5.2",
    "@playwright/test": "^1.53.2",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^24.0.10",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.2.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.30.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "vue-tsc": "^3.0.1",
    "wrangler": "4.23.0"
  },
  "pnpm": {
    "overrides": {
      "@intlify/message-compiler": "^11.1.2",
      "chokidar": "3.6.0"
    },
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "maplibre-gl",
      "sharp",
      "unrs-resolver",
      "vue-demi",
      "workerd"
    ],
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "maplibre-gl",
      "sharp",
      "unrs-resolver",
      "vue-demi",
      "workerd"
    ]
  },
  "lint-staged": {
    "**/*": "eslint"
  }
}
